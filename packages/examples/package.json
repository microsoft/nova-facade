{
  "name": "@nova/examples",
  "version": "1.11.2",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "build": "monorepo-scripts build",
    "build-storybook": "storybook build",
    "generate": "graphql-codegen --config codegen.yml && yarn generate:apollo && yarn generate:relay",
    "generate:apollo": "duct-tape-compiler --emitDocuments --schema ./data/schema.graphql --src ./src/apollo --watchman false",
    "generate:relay": "relay-compiler",
    "just": "monorepo-scripts",
    "lint": "monorepo-scripts lint",
    "prepare": "yarn generate",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "types": "monorepo-scripts types"
  },
  "dependencies": {
    "@nova/react": "2.9.0",
    "@nova/react-test-utils": "7.2.1",
    "@nova/types": "1.7.0",
    "graphql": "^15.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-relay": "^20.0.0",
    "relay-hooks": "^9.4.0",
    "relay-runtime": "^20.0.0"
  },
  "devDependencies": {
    "@graphitation/apollo-react-relay-duct-tape": "^1.3.0",
    "@graphitation/apollo-react-relay-duct-tape-compiler": "^1.6.0",
    "@graphitation/graphql-codegen-typescript-typemap-plugin": "^0.1.9",
    "@graphitation/graphql-js-tag": "^0.9.4",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-resolvers": "^3.1.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/types": "^8.6.14",
    "@swc/core": "^1.11.0",
    "@swc/plugin-relay": "^7.0.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-relay": "^18.0.0",
    "@types/relay-runtime": "^19.0.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "monorepo-scripts": "*",
    "playwright": "^1.51.1",
    "relay-compiler": "^20.0.0",
    "relay-test-utils": "^20.0.0",
    "storybook": "^8.6.14",
    "typescript": "^5.8.0",
    "vite": "^6.3.0",
    "vite-plugin-graphql-loader": "^4.0.0",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^0.1.1"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.mjs",
        "require": "./lib/index.js"
      }
    },
    "main": "./lib/index",
    "module": "./lib/index.mjs",
    "types": "./lib/index.d.ts"
  },
  "access": "public"
}
